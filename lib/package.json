{
  "name": "parinfer",
  "version": "3.1.1",
  "description": "a simpler way to write Lisp",
  "main": "parinfer.js",
  "scripts": {
    "test": "eslint parinfer.js && eslint test.js && mocha"
  },
  "files": [
    "parinfer.js",
    "test.js",
    "README.md",
    "CHANGES.md"
  ],
  "eslintConfig": {
    "extends": [
      "defaults"
    ],
    "env": {
      "node": true,
      "commonjs": true,
      "amd": true,
      "browser": true
    },
    "rules": {
      "no-console": 0,
      "no-constant-condition": 0,
      "semi": 2
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunlebron/parinfer"
  },
  "keywords": [
    "parinfer",
    "paredit",
    "lisp"
  ],
  "author": "Shaun LeBron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shaunlebron/parinfer/issues"
  },
  "homepage": "http://shaunlebron.github.io/parinfer",
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^7.1.1",
    "mocha": "^2.3.4"
  }
}
